{
  "name": "koorie",
  "version": "1.17.12-alpha",
  "license": "Apache-2.0",
  "description": "Node.js server. ESModule asynchronous from the beginning of the Dreamtime.",
  "type": "module",
  "main": "index.js",
  "bin": {
    "koorie": "koorie.js",
    "koorie-shell": "shell.js"
  },
  "engines": {
    "node": "^17 || ^16 || ^14.15 || ^12.13"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "test": "npx 4t unit --exclude=koorie_log_writer.bin.test.js",
    "coverage": "npx 4t --coverage unit --exclude=koorie_log_writer.bin.test.js",
    "nodemon.koorie": "npx nodemon --trace-uncaught --trace-warnings koorie.js --static-files=public --port=8080",
    "nodemon.ssl.koorie": "npx nodemon --trace-uncaught --trace-warnings koorie.js --static-files=public --port=4443 --secure=\"'options(active:true|key:certs/koorie.key.pem|cert:certs/koorie.cert.pem|dhparam:certs/koorie.dhparam.pem)'\"",
    "nodemon.http2.koorie": "npx nodemon --trace-uncaught --trace-warnings koorie.js --static-files=public --http2 --port=4444 --secure=\"'options(active:true|key:certs/koorie.key.pem|cert:certs/koorie.cert.pem|dhparam:certs/koorie.dhparam.pem)'\""
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "autocannon": "^7.9.0",
    "eslint": "^8.15.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "nodemon": "^2.0.16",
    "trythistrythat": "^1.8.9"
  },
  "dependencies": {
    "@cli-blaze/activity": "^1.0.3",
    "@cli-blaze/decors": "^1.0.3",
    "@cli-blaze/dissection": "^1.0.0",
    "@cli-blaze/error": "^1.0.2",
    "@cli-blaze/input": "^1.0.5",
    "boolean-jokes": "^1.5.5",
    "json-swiss-knife": "^1.6.14",
    "oftypes": "^1.8.2",
    "semver": "^7.3.7",
    "validate-npm-package-name": "^4.0.0"
  },
  "keywords": [
    "server",
    "async",
    "esmodule",
    "middlewares"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
  "homepage": "https://github.com/simonedelpopolo/koorie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/koorie.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/koorie/issues"
  }
}
