{
  "name": "koorie",
  "version": "1.0.0-experimental",
  "description": "Node.js server. ESM Module asynchronous from the beginning of the Dreamtime.",
  "type": "module",
  "engines": {
    "node": ">=v16.13.1 <17"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "main": "index.js",
  "bin": {
    "koorie": "koorie.js",
    "koorie-shell": "shell.js"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "autocannon": "4t test --file='options(filename:autocannon.test.js)'",
    "nodemon.koorie": "nodemon --trace-warnings koorie.js -s=assertions/public --hot"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autocannon": "^7.6.0",
    "eslint": "^8.9.0",
    "eslint-plugin-jsdoc": "^37.9.1",
    "nodemon": "^2.0.15",
    "trythistrythat": "^0.0.4"
  },
  "dependencies": {
    "boolean-jokes": "^1.4.7",
    "json-swiss-knife": "^1.5.9",
    "oftypes": "^0.4.4"
  },
  "homepage": "https://github.com/simonedelpopolo/koorie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/koorie.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/koorie/issues"
  },
  "keywords": [
    "server",
    "async",
    "esmmodule",
    "module",
    "middleware",
    "promise"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
