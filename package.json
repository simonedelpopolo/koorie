{
  "name": "koorie",
  "version": "1.5.0-experimental",
  "description": "Node.js server. ESModule asynchronous from the beginning of the Dreamtime.",
  "type": "module",
  "engines": {
    "node": ">=v16.14.0 <17"
  },
  "main": "index.js",
  "bin": {
    "koorie": "koorie.js",
    "koorie-shell": "shell.js"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "unit-tests": "npx 4t unit",
    "autocannon-test": "npx 4t test --file='options(filename:autocannon.test.js)'",
    "middleware-flag-test": "npx 4t test --file='options(filename:middleware_flag.test.js)'",
    "static-files-flag-test": "npx 4t test --file='options(filename:static_files_flag.test.js)'",
    "nodemon.koorie": "npx nodemon --trace-warnings koorie.js --response-time=true -s=public --hot --socket=\"'options(active:true:path:/tmp/koorie.sock)'\" -c=3",
    "nodemon.ssl.koorie": "npx nodemon --trace-warnings koorie.js --port=3302 --response-time=true -s=public --hot --socket=\"'options(active:true:path:/tmp/koorie.ssl.sock)'\" -c=3 --secure=\"'options(active:true:key:certs/koorie.key.pem:cert:certs/koorie.cert.pem:dhparam:certs/koorie.dhparam.pem)'\""
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autocannon": "^7.7.0",
    "eslint": "^8.11.0",
    "eslint-plugin-jsdoc": "^38.0.2",
    "nodemon": "^2.0.15",
    "trythistrythat": "^0.1.25"
  },
  "dependencies": {
    "boolean-jokes": "^1.5.1",
    "json-swiss-knife": "^1.6.10",
    "oftypes": "^0.7.0"
  },
  "homepage": "https://github.com/simonedelpopolo/koorie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/koorie.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/koorie/issues"
  },
  "keywords": [
    "server",
    "async",
    "esmmodule",
    "module",
    "middleware",
    "promise"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
  "license": "Apache-2.0"
}
