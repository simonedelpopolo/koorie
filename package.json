{
  "name": "koorie",
  "version": "1.16.16-alpha",
  "license": "Apache-2.0",
  "description": "Node.js server. ESModule asynchronous from the beginning of the Dreamtime.",
  "type": "module",
  "main": "public.js",
  "bin": {
    "koorie": "koorie.js",
    "koorie-shell": "shell.js"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "unit-tests": "npx 4t unit",
    "autocannon-test": "npx 4t test --file='options(filename:autocannon.test.js)'",
    "middleware-flag-test": "npx 4t test --file='options(filename:middleware_flag.test.js)'",
    "static-files-flag-test": "npx 4t test --file='options(filename:static_files_flag.test.js)'",
    "nodemon.koorie": "npx nodemon --trace-uncaught --trace-warnings koorie.js --static-files=public --port=8080",
    "nodemon.ssl.koorie.socket.hot": "npx nodemon --trace-uncaught --trace-warnings koorie.js --port=4443 --static-files=public --hot --socket=\"'options(active:true|path:/tmp/koorie.ssl.sock)'\" --cluster=3 --secure=\"'options(active:true|key:certs/koorie.key.pem|cert:certs/koorie.cert.pem|dhparam:certs/koorie.dhparam.pem)'\""
  },
  "devDependencies": {
    "autocannon": "^7.8.1",
    "eslint": "^8.12.0",
    "eslint-plugin-jsdoc": "^38.1.6",
    "nodemon": "^2.0.15",
    "trythistrythat": "^1.2.4"
  },
  "dependencies": {
    "boolean-jokes": "^1.5.4",
    "cli-decors": "^1.0.1",
    "json-swiss-knife": "^1.6.13",
    "oftypes": "^1.7.1",
    "semver": "^7.3.5",
    "validate-npm-package-name": "^4.0.0"
  },
  "keywords": [
    "server",
    "async",
    "esmodule",
    "module",
    "middleware",
    "promise"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
  "homepage": "https://github.com/simonedelpopolo/koorie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/koorie.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/koorie/issues"
  }
}
