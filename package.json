{
  "name": "koorie",
  "version": "0.4.85-experimental",
  "description": "Node.js server. ESM Module asynchronous from the beginning of the Dreamtime.",
  "type": "module",
  "engines": {
    "node": ">=v16.13.1 <17"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "main": "index.js",
  "bin": {
    "koorie": "koorie.js",
    "koorie-shell": "shell.js"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "tests": "cd assertions && node assertions.js",
    "autocannon": "cd assertions && node assertions.js autocannon",
    "koorie": "node koorie.js",
    "nodemon.koorie": "nodemon --trace-warnings koorie.js -s=public",
    "nodemon.koorie-cluster-react": "npx nodemon --trace-warnings koorie.js --cluster --static-files=public --port=2654 --address=localhost --react",
    "nodemon.koorie-react": "npx nodemon --trace-warnings koorie.js --port=8080 --address=localhost --static-files=public --react"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autocannon": "^7.6.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jsdoc": "^37.7.1",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "boolean-jokes": "latest",
    "json-swiss-knife": "latest",
    "oftypes": "latest"
  },
  "homepage": "https://github.com/simonedelpopolo/koorie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonedelpopolo/koorie.git"
  },
  "bugs": {
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo/koorie/issues"
  },
  "keywords": [
    "server",
    "async",
    "esmmodule",
    "module",
    "middleware",
    "promise"
  ],
  "author": {
    "name": "Simone Del Popolo",
    "email": "simonedelpopolo@outlook.com",
    "url": "https://github.com/simonedelpopolo"
  },
  "license": "Apache-2.0"
}
