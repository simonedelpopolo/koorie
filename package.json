{
  "name": "koorie",
  "version": "0.4.25-alpha",
  "description": "Node.js server.",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postinstall" : "node ./pkg/install.js",
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "nodemon.koorie": "node executable.js",
    "nodemon.koorie-cluster-json-react": "npx nodemon executable.js -c='{\"cpus\":8,\"init\":\"default\"}' --static-files=public --port=2654 --address=localhost --react",
    "nodemon.koorie-react": "npx nodemon --trace-warnings executable.js --port=8080 --address=localhost --static-files=public -r"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-plugin-jsdoc": "^37.6.1",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "boolean-jokes": "latest",
    "json-swiss-knife": "latest",
    "oftypes": "latest"
  },
  "author": "Simone Del Popolo",
  "license": "Apache-2.0"
}
